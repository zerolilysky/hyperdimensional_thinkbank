{
  "name": "hyperdimensional-thinkbank",
  "version": "1.0.0",
  "description": "超维智库平台项目",
  "scripts": {
    "dev:server": "ts-node-dev --respawn --transpileOnly server/index.ts",
    "build:server": "tsc -p tsconfig.json",
    "start:server": "node dist/server/index.js",
    "dev:client": "react-scripts start",
    "build:client": "react-scripts build",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^7.6.0",
    "react-scripts": "5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
